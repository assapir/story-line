{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-var-requires": false,
        "no-console": false,
        "variable-name": false,
        "quotemark": [true, "backtick", "avoid-escape"],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "src/migrations/*.ts",
            "lib/**/*",
            "coverage/**/*",
            "ormconfig.js"
        ]
    }
}